<?xml version="1.0" ?>
<!DOCTYPE application PUBLIC "-//Sun Microsystems, Inc.//DTD J2EE Application 1.3//EN" "http://java.sun.com/dtd/application_1_3.dtd">
<application id="CoffeeShopApplication">
    <display-name>Coffee Shop</display-name>
    <module>
        <web>
            <web-uri>CoffeeShopWeb.war</web-uri>
            <context-root>coffeeShop</context-root>
        </web>
    </module>
    <module>
        <web>
            <web-uri>CoffeeShopInContainerTests.war</web-uri>
            <context-root>CoffeeShopInContainerTests</context-root>
        </web>
    </module>
    <module>
        <ejb>CoffeeShopEJB.jar</ejb>
    </module>
    <module>
        <ejb>CoffeeShopLegacyEJB.jar</ejb>
    </module>
</application>
